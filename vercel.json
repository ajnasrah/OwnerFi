{
  "framework": "nextjs",
  "buildCommand": "npm run build",
  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "regions": ["iad1"],
  "git": {
    "deploymentEnabled": {
      "main": true
    }
  },
  "env": {
    "NODE_ENV": "production"
  },
  "crons": [
    {
      "path": "/api/cron/generate-videos",
      "schedule": "0 9,12,15,18,21 * * *"
    },
    {
      "path": "/api/cron/fetch-rss",
      "schedule": "0 12 * * *"
    },
    {
      "path": "/api/cron/rate-articles",
      "schedule": "0 13 * * *"
    },
    {
      "path": "/api/benefit/cron",
      "schedule": "20 9,12,15,18,21 * * *"
    },
    {
      "path": "/api/property/video-cron",
      "schedule": "40 9,12,15,18,21 * * *"
    },
    {
      "path": "/api/cron/check-google-drive",
      "schedule": "0 7,11,15,19 * * *"
    },
    {
      "path": "/api/cron/check-stuck-workflows",
      "schedule": "*/30 14-23,0-4 * * *"
    },
    {
      "path": "/api/cron/weekly-maintenance",
      "schedule": "0 11 * * 1"
    },
    {
      "path": "/api/cron/daily-maintenance",
      "schedule": "0 3 * * *"
    },
    {
      "path": "/api/cron/refill-articles",
      "schedule": "0 */6 * * *"
    },
    {
      "path": "/api/cron/process-scraper-queue",
      "schedule": "0 10,12,14,16,18,20,22 * * *"
    },
    {
      "path": "/api/benefit/workflow/auto-retry",
      "schedule": "0 */2 * * *"
    },
    {
      "path": "/api/cron/abdullah",
      "schedule": "0 9,12,15,18,21 * * *"
    },
    {
      "path": "/api/cron/sync-property-queue",
      "schedule": "0 8-20 * * *"
    },
    {
      "path": "/api/cron/generate-blog",
      "schedule": "0 11 * * *"
    }
  ],
  "functions": {
    "src/app/api/cron/process-scraper-queue/route.ts": {
      "maxDuration": 300
    },
    "src/app/api/admin/upload-properties-v4/route.ts": {
      "maxDuration": 300
    },
    "src/app/api/admin/scraper/upload/route.ts": {
      "maxDuration": 30
    },
    "src/app/api/**/*.ts": {
      "maxDuration": 30
    },
    "src/app/api/process-video/route.ts": {
      "maxDuration": 300
    },
    "app/api/podcast/generate/route.ts": {
      "maxDuration": 300
    },
    "src/app/api/cron/fetch-rss/route.ts": {
      "maxDuration": 300
    },
    "src/app/api/cron/rate-articles/route.ts": {
      "maxDuration": 300
    },
    "src/app/api/cron/generate-videos/route.ts": {
      "maxDuration": 60
    },
    "src/app/api/cron/check-stuck-workflows/route.ts": {
      "maxDuration": 300
    },
    "src/app/api/cron/weekly-maintenance/route.ts": {
      "maxDuration": 60
    },
    "src/app/api/cron/daily-maintenance/route.ts": {
      "maxDuration": 300
    },
    "src/app/api/benefit/cron/route.ts": {
      "maxDuration": 60
    },
    "src/app/api/property/video-cron/route.ts": {
      "maxDuration": 60
    },
    "src/app/api/property/generate-video/route.ts": {
      "maxDuration": 60
    },
    "src/app/api/benefit/workflow/auto-retry/route.ts": {
      "maxDuration": 60
    },
    "src/app/api/benefit/workflow/retry/route.ts": {
      "maxDuration": 60
    },
    "src/app/api/cron/abdullah/route.ts": {
      "maxDuration": 60
    },
    "src/app/api/cron/sync-property-queue/route.ts": {
      "maxDuration": 300
    },
    "src/app/api/cron/check-google-drive/route.ts": {
      "maxDuration": 300
    },
    "src/app/api/cron/generate-blog/route.ts": {
      "maxDuration": 300
    }
  },
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        { "key": "Access-Control-Allow-Origin", "value": "*" },
        { "key": "Access-Control-Allow-Methods", "value": "GET, POST, PUT, DELETE, OPTIONS" },
        { "key": "Access-Control-Allow-Headers", "value": "Content-Type, Authorization" }
      ]
    }
  ],
  "redirects": [
    {
      "source": "/dashboard/setup",
      "has": [
        {
          "type": "query",
          "key": "type",
          "value": "buyer"
        }
      ],
      "destination": "/buyer/register",
      "permanent": false
    },
    {
      "source": "/dashboard/setup",
      "has": [
        {
          "type": "query",
          "key": "type", 
          "value": "realtor"
        }
      ],
      "destination": "/realtor/setup",
      "permanent": false
    }
  ]
}