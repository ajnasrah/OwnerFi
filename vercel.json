{
  "framework": "nextjs",
  "buildCommand": "npm run build",
  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "regions": ["iad1"],
  "git": {
    "deploymentEnabled": {
      "main": true
    }
  },
  "env": {
    "NODE_ENV": "production"
  },
  "crons": [
    {
      "path": "/api/cron/run-agent-outreach-scraper",
      "schedule": "0 11 * * *"
    },
    {
      "path": "/api/cron/process-agent-outreach-queue",
      "schedule": "0 8,16,0 * * *"
    },
    {
      "path": "/api/cron/refresh-zillow-status",
      "schedule": "*/30 * * * *"
    },
    {
      "path": "/api/cron/generate-videos",
      "schedule": "0 9,12,15,18,21 * * *"
    },
    {
      "path": "/api/cron/fetch-rss",
      "schedule": "0 12 * * *"
    },
    {
      "path": "/api/cron/rate-articles",
      "schedule": "0 13 * * *"
    },
    {
      "path": "/api/benefit/cron",
      "schedule": "20 9,12,15,18,21 * * *"
    },
    {
      "path": "/api/cron/check-google-drive",
      "schedule": "0 7,11,15,19 * * *"
    },
    {
      "path": "/api/cron/check-stuck-workflows",
      "schedule": "*/30 14-23,0-4 * * *"
    },
    {
      "path": "/api/cron/daily-maintenance",
      "schedule": "0 3 * * *"
    },
    {
      "path": "/api/cron/refill-articles",
      "schedule": "0 */6 * * *"
    },
    {
      "path": "/api/cron/abdullah",
      "schedule": "0 9,12,15,18,21 * * *"
    },
    {
      "path": "/api/cron/generate-blog",
      "schedule": "0 11 * * *"
    },
    {
      "path": "/api/v2/scraper/run",
      "schedule": "0 12 * * *"
    }
  ],
  "functions": {
    "src/app/api/admin/upload-properties-v4/route.ts": {
      "maxDuration": 300
    },
    "src/app/api/process-video/route.ts": {
      "maxDuration": 300
    },
    "src/app/api/cron/fetch-rss/route.ts": {
      "maxDuration": 300
    },
    "src/app/api/cron/rate-articles/route.ts": {
      "maxDuration": 300
    },
    "src/app/api/cron/generate-videos/route.ts": {
      "maxDuration": 60
    },
    "src/app/api/cron/check-stuck-workflows/route.ts": {
      "maxDuration": 300
    },
    "src/app/api/cron/daily-maintenance/route.ts": {
      "maxDuration": 300
    },
    "src/app/api/property/video-cron/route.ts": {
      "maxDuration": 60
    },
    "src/app/api/property/video-cron-spanish/route.ts": {
      "maxDuration": 60
    },
    "src/app/api/cron/abdullah/route.ts": {
      "maxDuration": 60
    },
    "src/app/api/cron/check-google-drive/route.ts": {
      "maxDuration": 300
    },
    "src/app/api/cron/generate-blog/route.ts": {
      "maxDuration": 300
    },
    "src/app/api/cron/refresh-zillow-status/route.ts": {
      "maxDuration": 300
    },
    "src/app/api/cron/run-agent-outreach-scraper/route.ts": {
      "maxDuration": 300
    },
    "src/app/api/cron/process-agent-outreach-queue/route.ts": {
      "maxDuration": 300
    },
    "src/app/api/workers/process-video/route.ts": {
      "maxDuration": 300
    },
    "src/app/api/webhooks/submagic/[brand]/route.ts": {
      "maxDuration": 300
    },
    "src/app/api/webhooks/heygen/[brand]/route.ts": {
      "maxDuration": 60
    },
    "src/app/api/v2/scraper/run/route.ts": {
      "maxDuration": 600
    },
    "src/app/api/v2/scraper/add/route.ts": {
      "maxDuration": 120
    }
  },
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        { "key": "Access-Control-Allow-Origin", "value": "*" },
        { "key": "Access-Control-Allow-Methods", "value": "GET, POST, PUT, DELETE, OPTIONS" },
        { "key": "Access-Control-Allow-Headers", "value": "Content-Type, Authorization" }
      ]
    }
  ],
  "redirects": [
    {
      "source": "/dashboard/setup",
      "has": [
        {
          "type": "query",
          "key": "type",
          "value": "buyer"
        }
      ],
      "destination": "/buyer/register",
      "permanent": false
    },
    {
      "source": "/dashboard/setup",
      "has": [
        {
          "type": "query",
          "key": "type", 
          "value": "realtor"
        }
      ],
      "destination": "/realtor/setup",
      "permanent": false
    }
  ]
}